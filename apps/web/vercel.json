{
  "framework": "nextjs",
  "installCommand": "cd ../.. && npm install",
  "rewrites": [
    {
      "source": "/api/:path*",
      "destination": "https://runesmithweb-production.up.railway.app/api/:path*"
    }
  ],
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "X-Frame-Options",
          "value": "DENY"
        },
        {
          "key": "X-Content-Type-Options",
          "value": "nosniff"
        },
        {
          "key": "Referrer-Policy",
          "value": "strict-origin-when-cross-origin"
        }
      ]
    }
  ]
}
